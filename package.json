{
  "name": "eventspotter-frontend",
  "scripts": {
    "dev": "concurrently --names \"FRONTEND,BACKEND\" -c \"blue,red\" --prefix \"{name} \" \"npm run dev:fe\" \"npm run dev:be\" ",
    "dev:fe": "NODE_ENV=development FRONTEND_HOST=http://localhost:3000 nodemon server.js",
    "dev:be": "cd ../eventspotter-backend && npm run start:dev",
    "staging": "NODE_ENV=staging FRONTEND_HOST=http://localhost:3000 nodemon server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "btoa": "^1.2.1",
    "cacheable-response": "^1.6.10",
    "concurrently": "^4.1.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "date-fns": "^1.30.1",
    "dotenv": "^8.0.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "lru-cache": "^5.1.1",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "next": "^9.0.3",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-table": "^7.0.0-alpha.15",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.10.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.13.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1"
  },
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/setUpTests.js"
    ]
  }
}
